<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.cenp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <name>CENP</name>
    <description>
        CENP - Revista
    </description>
    <author email="programador@clarearpropaganda.com.br" href="http://clarearpropaganda.com.br">
        Clarear Propaganda
    </author>
    <icon src="icon.png" />
    <content src="index.html" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="phonegap-version" value="cli-6.3.0" />
    <preference name="android-build-tool" value="gradle" />
    <access origin="*" />
    <access origin="*.pushbots.com" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
