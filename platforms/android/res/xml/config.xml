<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.cenp" ios-CFBundleVersion="4.0.0" version="1.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>CENP</name>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <name>CENP Revista</name>
    <description>
        CENP - Revista
    </description>
    <author email="programador@clarearpropaganda.com.br" href="http://clarearpropaganda.com.br">
        Clarear Propaganda
    </author>
    <icon src="icon.png" />
    <splash src="splash.png" />
    <icon height="180" platform="ios" src="www/res/icons/ios/icon-60@3x.png" width="180" />
    <icon height="60" platform="ios" src="www/res/icons/ios/icon-60.png" width="60" />
    <icon height="120" platform="ios" src="www/res/icons/ios/icon-60@2x.png" width="120" />
    <icon height="76" platform="ios" src="www/res/icons/ios/icon-76.png" width="76" />
    <icon height="152" platform="ios" src="www/res/icons/ios/icon-76@2x.png" width="152" />
    <icon height="29" platform="ios" src="www/res/icons/ios/icon-small.png" width="29" />
    <icon height="58" platform="ios" src="www/res/icons/ios/icon-small@2x.png" width="58" />
    <icon height="87" platform="ios" src="www/res/icons/ios/icon-small@3x.png" width="87" />
    <icon height="40" platform="ios" src="www/res/icons/ios/icon-40.png" width="40" />
    <icon height="80" platform="ios" src="www/res/icons/ios/icon-40@2x.png" width="80" />
    <icon height="120" platform="ios" src="www/res/icons/ios/icon-40@3x.png" width="120" />
    <icon platform="android" qualifier="ldpi" src="www/res/icons/android/icon-36-ldpi.png" />
    <icon platform="android" qualifier="mdpi" src="www/res/icons/android/icon-48-mdpi.png" />
    <icon platform="android" qualifier="hdpi" src="www/res/icons/android/icon-72-hdpi.png" />
    <icon platform="android" qualifier="xhdpi" src="www/res/icons/android/icon-96-xhdpi.png" />
    <icon platform="android" qualifier="xxhdpi" src="www/res/icons/android/icon-144-xxhdpi.png" />
    <icon platform="android" qualifier="fr-xxhdpi" src="www/res/icons/android/icon-192-xxxhdpi.png" />
    <content src="index.html" />
    <preference name="android-minSdkVersion" value="14" />
    <gap:plugin name="pushbots-cordova-plugin" source="npm" spec="^1.4.0" />
    <preference name="phonegap-version" value="cli-6.3.0" />
    <preference name="android-build-tool" value="gradle" />
    <access origin="*" />
    <access origin="*.pushbots.com" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
